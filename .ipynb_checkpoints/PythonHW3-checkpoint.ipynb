    "# function does Monte Carlo simulations\n",
    "def MC_sim(n):\n",
    "    U = []\n",
    "    for i in range(n):\n",
    "        x = np.random.random_sample()\n",
    "        U.append(4 * np.sqrt(1 - np.power(x,2)))\n",
    "    return np.mean(U)"
